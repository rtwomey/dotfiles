[core]
  whitespace = trailing-space,space-before-tab
	excludesfile = /Users/rtwomey/.gitignore
[user]
  name = Ryan Twomey
  email = rtwomey@gmail.com
[color]
  diff = auto
  status = auto
  branch = auto
  interactive = auto
  ui = true
  pager = true
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = blue bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[alias]
  co = checkout
  s = status
  pr = pull --rebase
  l = log --graph --all --pretty=format:'%C(red)%h%C(reset) %C(yellow)%d%C(reset)%s %C(green)%an %C(bold black)%cr%C(reset)'
  lol = log --pretty=oneline --abbrev-commit --graph --decorate
  last = log -1
  stats = shortlog -s --
  unstage = reset HEAD
  staged = diff --cached
  unstaged = diff
  current-branch = !git symbolic-ref -q HEAD | sed -e 's|^refs/heads/||'

